public class occurance {
    public static void main(String[]args){
        int a[]= {2,2,4,5,6,6,7,7};
        int n= a.length;
        int cnt=0;
        Scanner sc= new Scanner(System.in);

        System.out.println("enter to check occurence");
        int b= sc.nextInt();
        for(int i=0; i<n;i++){
             if( a[i]==b){
                 cnt++;
             }
        }
        System.out.println("elemnt"+" " +b+"is occured"+cnt);
    }
}
